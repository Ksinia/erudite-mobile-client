{
  "cli": {
    "version": ">= 16.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "development",
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "ios": {
        "credentialsSource": "local",
        "image": "latest"
      },
      "channel": "preview",
      "environment": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "environment": "production",
      "ios": {
        "image": "latest"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "channel": "development-simulator",
      "environment": "development"
    }
  },
  "submit": {
    "production": {}
  }
}
